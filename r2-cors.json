{
  "rules": [
    {
      "allowed": {
        "origins": [
          "http://localhost:3000"
        ],
        "methods": [
          "GET",
          "PUT",
          "POST",
          "DELETE",
          "HEAD"
        ],
        "headers": [
          "Authorization",
          "Content-Type",
          "Content-Length",
          "Content-MD5",
          "x-amz-date",
          "x-amz-content-sha256"
        ]
      }
    }
  ]
}